<h1>Listing divs</h1>

<table>
  <thead>
    <tr>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @divs.each do |div| %>
      <tr>
        <td><%= link_to 'Show', div %></td>
        <td><%= link_to 'Edit', edit_div_path(div) %></td>
        <td><%= link_to 'Destroy', div, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

